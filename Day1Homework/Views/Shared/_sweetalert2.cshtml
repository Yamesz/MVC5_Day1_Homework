@{ 
    if (ViewData["AlertViewModel"] == null)
    {
        return;
    }

    AlertViewModel Model = ViewData["AlertViewModel"] as AlertViewModel;

    if (Model == null || string.IsNullOrWhiteSpace(Model.Title))
    {
        return;
    }
}

<script>
    swal(
      '@Model.Title',
      '@Model.Msg',
      '@Model.Status'
    );
</script>
